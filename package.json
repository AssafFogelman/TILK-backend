{
  "name": "tilk-backend-server",
  "type": "module",
  "version": "1.0.0",
  "description": "tilk-backend-server",
  "main": "index.js",
  "scripts": {
    "dev": "tsx src/index.ts",
    "dev2": "nodemon --quiet --exec \"node --import tsx src/main.ts\"",
    "db:generate": "npx drizzle-kit generate:pg",
    "db:migrate": "node --import tsx src/drizzle/migrate.ts",
    "db:execute": "nodemon --quiet --exec \"node --import tsx src/drizzle/main.ts\"",
    "db:upgrade": "npx drizzle-kit up:pg",
    "db:drop": "npx drizzle-kit drop"
  },
  "author": "Assaf Fogelman",
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.17.17",
    "@types/pg": "^8.11.10",
    "drizzle-kit": "^0.20.17",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@hono/node-server": "^1.11.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.30.8",
    "expo-server-sdk": "^3.13.0",
    "hono": "^4.4.13",
    "jsonwebtoken": "^9.0.2",
    "latest": "^0.2.0",
    "pg": "^8.13.0",
    "postgres": "^3.4.4",
    "sharp": "^0.33.4",
    "socket.io": "^4.7.5",
    "twilio": "^5.1.0",
    "zod": "^3.23.5"
  }
}
